[global_config]
  tab_position = hidden
  inactive_color_offset = 0.92
[keybindings]
  switch_to_tab_10 = None
  new_tab = <Shift>Down
  next_tab = <Shift>Right
  move_tab_right = <Primary>Right
  prev_tab = <Shift>Left
  switch_to_tab_4 = <Primary>4
  switch_to_tab_5 = <Primary>5
  move_tab_left = <Primary>Left
  switch_to_tab_3 = <Primary>3
  switch_to_tab_1 = <Primary>1
  switch_to_tab_2 = <Primary>2
  copy = <Primary><Alt>c
  paste = <Primary><Alt>v
[profiles]
  [[default]]
    scrollbar_position = hidden
    palette = "#1e2320:#705050:#60b48a:#dfaf8f:#506070:#ad5693:#8cd0d3:#dcdccc:#709080:#dca3a3:#c3bf9f:#f0dfaf:#94bff3:#913f7b:#93e0e3:#ffffff"
    background_image = None
    scrollback_lines = 10000
    cursor_shape = underline
    use_system_font = False
    foreground_color = "#93b9a0"
    show_titlebar = False
    font = Source Code Pro Medium 10
    background_color = "#212020"
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
      command = ""
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
